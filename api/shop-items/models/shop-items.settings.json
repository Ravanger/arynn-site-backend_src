{
  "kind": "collectionType",
  "collectionName": "shop_items",
  "info": {
    "name": "Shop Items",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": false
    },
    "price": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "prints",
        "stickers",
        "goods",
        "custom",
        "originals"
      ],
      "required": true
    },
    "maxquantity": {
      "type": "integer",
      "default": 23,
      "min": 1
    },
    "is_sold": {
      "type": "boolean",
      "default": false
    },
    "custom_types": {
      "type": "component",
      "repeatable": true,
      "component": "custom.custom-types"
    },
    "custom_addons": {
      "type": "component",
      "repeatable": true,
      "component": "custom.custom-addons"
    },
    "custom_numberofpeople": {
      "type": "component",
      "repeatable": true,
      "component": "custom.custom-numberofpeople"
    }
  }
}
